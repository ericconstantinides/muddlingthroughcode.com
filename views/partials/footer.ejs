<footer class="site-footer">
  <div class="skinny-container site-footer__inner">
    <div class="footer-links">
      <a class="footer-links__link" href="https://github.com/ericconstantinides" title="GitHub" data-print-url="github.com/ericconstantinides"><img class="footer-links__image" src="data:image/png;base64," alt="GitHub logo"></a>
      <a class="footer-links__link" href="/rss" title="RSS Feed"><img class="footer-links__image" src="/images/rss.png"></a>
    </div>
    <div class="about">
      <div class="about__image-container m-ratio m-ratio--1-1 js-scroll-activation" data-activate-this-at-bottom>
        <div class="m-ratio__child">
          <img class="eric" src="/images/eric-constantinides.jpg">
          <img class="monkey" src="<%- siteMeta.website.primaryImage.path %>">
        </div>
      </div>
      <% if (typeof about !== 'undefined') { -%>
      <div class="about__content">
        <%- about %>
      </div>
      <% } -%>
    </div>
    <p class="site-footer__copyright">
      &copy; <%- new Date().getFullYear() %> <a href="<%- siteMeta.owner.url%>"><%- siteMeta.owner.name%></a>
    </p>
  </div>
</footer>
